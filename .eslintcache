HangUpdate.splice(indexGH,1)\r\n        }else{\r\n            alert('Sản phẩm không tồn tại')\r\n        }\r\n\r\n        this.setState({\r\n            gioHang: gioHangUpdate\r\n        })\r\n    }\r\n\r\n    render() {\r\n        let {hinhAnh,tenSP,giaBan,manHinh,heDieuHanh,cameraSau,cameraTruoc} = this.state.spChiTiet\r\n        return (\r\n            <div className='container'>\r\n                <h3 className='p-2'>DANH SÁCH SẢN PHẨM</h3>\r\n                <GioHang gioHang={this.state.gioHang} xoaGioHang={this.xoaGioHang}/>\r\n                <div className='row'>\r\n                    {this.renderSanPham()}\r\n                </div>\r\n\r\n                <div className='row mt-3'>\r\n                    <div className='col-4'>\r\n                        <h3>{tenSP}</h3>\r\n                        <img className=\"card-img-top\" src={hinhAnh} alt />\r\n                    </div>\r\n                    <div className='col-8'>\r\n                        <table className='table text-left'>\r\n                            <thead>\r\n                                <h3>Thông số kĩ thuật</h3>\r\n                                <tr>\r\n                                    <td>Màn hình:</td>\r\n                                    <td>{manHinh}</td>\r\n                                </tr>\r\n                                <tr>\r\n                                    <td>Hệ diều hành:</td>\r\n                                    <td>{heDieuHanh}</td>\r\n                                </tr>\r\n                                <tr>\r\n                                    <td>Camera trước:</td>\r\n                                    <td>{cameraTruoc}</td>\r\n                                </tr>\r\n                                <tr>\r\n                                    <td>Camera sau:</td>\r\n                                    <td>{cameraSau}</td>\r\n                                </tr>\r\n                                <tr>\r\n                                    <t